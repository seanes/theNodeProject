{
  "name": "the-node-project",
  "version": "0.0.1",
  "description": "Events distribution manager",
  "main": "server.js",
  "scripts": {
    "start": "echo \"Error: no start implemented yet\" && exit 1",
    "dev": "NODE_ENV=development node babel-server.js",
    "devW": "SET NODE_ENV=development&& node babel-server.js",
    "test": "NODE_ENV=test mocha --compilers js:babel-core/register ./tests/*/*",
    "smoke-test": "nightwatch --tag smoke",
    "read-test": "nightwatch --tag read",
    "write-test": "nightwatch --tag write",
    "phone-test": "nightwatch --tag phone",
    "testW": "SET NODE_ENV=test&& mocha --compilers js:babel-core/register ./tests/*/*",
    "build": "NODE_ENV=production node -r babel-register node_modules/.bin/webpack --config=webpack.config.js",
    "buildW": "SET NODE_ENV=production node -r babel-register node_modules/.bin/webpack --config=webpack.config.js",
    "postinstall": "node ./install-selenium.js"
  },
  "bin": {
    "nightwatch": "./node_modules/nightwatch/bin/nightwatch"
  },
  "author": "Jørgen Brække & Sean Scully",
  "license": "MIT",
  "dependencies": {
    "async": "^2.1.4",
    "axios": "^0.15.3",
    "babel": "^6.5.2",
    "babel-cli": "^6.22.2",
    "babel-core": "^6.22.1",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-preset-stage-2": "^6.18.0",
    "babel-register": "^6.22.0",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.16.0",
    "connect-flash": "^0.1.1",
    "css-loader": "^0.26.1",
    "express": "^4.14.0",
    "express-session": "^1.15.0",
    "image-webpack-loader": "^3.1.0",
    "material-ui": "^0.16.7",
    "mongoose": "^4.9.7",
    "mongoose-validator": "^1.2.5",
    "node-sass": "^4.4.0",
    "nodemailer": "^2.7.2",
    "normalize.css": "^5.0.0",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.1",
    "react-redux": "^5.0.2",
    "react-router": "^3.0.2",
    "react-router-redux": "^4.0.7",
    "react-tap-event-plugin": "^2.0.1",
    "redux": "^3.6.0",
    "redux-logger": "^2.7.4",
    "redux-thunk": "^2.1.0",
    "sass-loader": "^4.1.1",
    "style-loader": "^0.13.1",
    "webpack": "^1.12.2"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "chai-http": "^3.0.0",
    "mocha": "^3.2.0",
    "nightwatch": "^0.9.12",
    "nodemon": "^1.11.0",
    "pre-commit": "^1.2.2",
    "react-hot-loader": "^1.3.1",
    "selenium-download": "^2.0.9",
    "selenium-webdriver": "^3.1.0",
    "webpack-dev-middleware": "^1.9.0",
    "webpack-dev-server": "^1.16.2",
    "webpack-hot-middleware": "^2.15.0"
  }
}
